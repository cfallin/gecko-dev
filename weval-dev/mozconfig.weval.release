# Build only the SpiderMonkey JS test shell
ac_add_options --target=wasm32-wasi
ac_add_options --enable-project=js
ac_add_options --enable-application=js
ac_add_options --without-system-zlib
ac_add_options --without-intl-api
ac_add_options --disable-jit
ac_add_options --disable-shared-js
ac_add_options --disable-shared-memory
ac_add_options --disable-tests
ac_add_options --disable-clang-plugin
ac_add_options --enable-jitspew
ac_add_options --enable-optimize=-O3
ac_add_options --enable-portable-baseline-interp
ac_add_options --enable-portable-baseline-interp-force
ac_add_options --enable-aot-ics
ac_add_options --enable-aot-ics-force
ac_add_options --enable-pbl-weval
ac_add_options --enable-js-shell-wizer
ac_add_options --prefix=obj-weval-release/dist
mk_add_options MOZ_OBJDIR=obj-weval-release
